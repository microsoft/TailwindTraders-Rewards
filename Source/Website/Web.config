        type="System.Web.Handlers.ScriptModule, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
    </httpModules>
    <authentication mode="Forms">
      <forms name=".ASPXFORMSDEMO" loginUrl="Login.aspx"
      protection="All" path="/" timeout="360" defaultUrl="Admin.aspx" />
    </authentication>
    <!--<authorization>
      <deny users ="?" />
      <allow users = "*" />
    </authorization>-->
